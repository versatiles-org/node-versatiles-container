{
  "name": "@versatiles/container",
  "version": "1.2.0",
  "description": "A client library for reading VersaTiles containers",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "check": "npm run lint && npm run build && npm run test",
    "doc": "npx vrt ts2md src/index.ts tsconfig.build.json | npx vrt insertmd README.md '# API' true",
    "lint": "eslint . --color",
    "prepack": "npm run build && npm run doc",
    "release": "npx vrt release-npm",
    "test-coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "upgrade": "npm-check-updates -u && rm -f package-lock.json; rm -rf node_modules; npm i && npm update"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/versatiles-org/node-versatiles.git"
  },
  "homepage": "https://github.com/versatiles-org/node-versatiles/blob/main/versatiles-container/README.md",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "author": "yetzt <node@yetzt.me>, Michael Kreil <versatiles@michael-kreil.de>",
  "license": "Unlicense",
  "engines": {
    "node": ">= 18"
  },
  "keywords": [
    "cloudtiles",
    "geo",
    "gis",
    "map",
    "maps",
    "mapserver",
    "maptiles",
    "mvt",
    "opencloudtiles",
    "osm",
    "pbf",
    "tile",
    "tiles",
    "tileserver",
    "vectortiles",
    "versatiles"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.20",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@versatiles/release-tool": "^1.2.2",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "npm-check-updates": "^16.14.15",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
