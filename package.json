{
  "name": "@versatiles/container",
  "version": "1.4.0",
  "description": "A client library for reading VersaTiles containers",
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json && npm run doc-graph",
    "check": "npm run format:check && npm run lint && npm run build && npm run test",
    "doc": "npm run doc-typescript && npm run doc-graph",
    "doc-typescript": "vrt doc-typescript -f html",
    "doc-graph": "vrt deps-graph | vrt doc-insert README.md '## Dependency Graph'",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "lint": "eslint . --color",
    "prepack": "npm run build",
    "release": "vrt release-npm",
    "test-coverage": "vitest --run --coverage",
    "test": "vitest --run",
    "upgrade": "vrt deps-upgrade"
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/versatiles-org/node-versatiles-container.git"
  },
  "homepage": "https://github.com/versatiles-org/node-versatiles-container",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "author": "yetzt <node@yetzt.me>, Michael Kreil <versatiles@michael-kreil.de>",
  "license": "Unlicense",
  "engines": {
    "node": ">= 18"
  },
  "keywords": [
    "cloudtiles",
    "geo",
    "gis",
    "map",
    "maps",
    "mapserver",
    "maptiles",
    "mvt",
    "opencloudtiles",
    "osm",
    "pbf",
    "tile",
    "tiles",
    "tileserver",
    "vectortiles",
    "versatiles"
  ],
  "devDependencies": {
    "@types/node": "^25.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@versatiles/release-tool": "^2.7.1",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.8.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vitest": "^4.0.18"
  }
}
